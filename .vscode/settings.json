{
  "emmet.includeLanguages": {
    "nunjucks": "html"
  },
  "[nunjucks]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "okitavera.vscode-nunjucks-formatter",
  },
  "cSpell.words": [
    "autoplace",
    "beautify",
    "eleventy",
    "gsap",
    "gsapdemo",
    "gulpfile",
    "Kaku",
    "mixins",
    "mqpacker",
    "stylelint",
    "vite"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  }
}
